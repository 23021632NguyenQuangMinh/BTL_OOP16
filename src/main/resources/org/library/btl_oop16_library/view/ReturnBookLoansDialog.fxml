<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.library.btl_oop16_library.Controller.ReturnBookLendingDialogController">
    <children>
        <VBox alignment="CENTER" layoutY="27.0" prefHeight="82.0" prefWidth="380.0" spacing="10.0">
            <children>
                <TextField fx:id="IDField" prefHeight="25.0" prefWidth="285.0" promptText="ID">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <font>
                  <Font name="Montserrat Regular" size="14.0" />
               </font></TextField>
            <HBox prefHeight="100.0" prefWidth="200.0" spacing="30.0">
               <children>
                    <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#onCancelButtonClick" prefHeight="30.0" prefWidth="110.0" text="Cancel">
                     <font>
                        <Font name="Montserrat Regular" size="14.0" />
                     </font>
                  </Button>
                    <Button fx:id="confirmButton" mnemonicParsing="false" onAction="#onConfirmButtonClick" prefHeight="30.0" prefWidth="110.0" text="Confirm">
                     <font>
                        <Font name="Montserrat Regular" size="14.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
            </children>
         <padding>
            <Insets bottom="30.0" left="30.0" right="30.0" />
         </padding>
        </VBox>
    </children>
</AnchorPane>
