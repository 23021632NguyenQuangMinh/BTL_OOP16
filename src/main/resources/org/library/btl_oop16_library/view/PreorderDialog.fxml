<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="227.0" prefWidth="382.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.library.btl_oop16_library.Controller.PreorderDialogController">
    <children>
        <VBox layoutY="27.0" prefHeight="66.0" prefWidth="382.0" spacing="10.0">
            <children>
                <TextField fx:id="bookIDField" prefHeight="25.0" prefWidth="284.0" promptText="BookID" />
                <TextField fx:id="quantityField" prefHeight="25.0" prefWidth="284.0" promptText="Quantity" />
            </children>
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" />
            </padding>
        </VBox>
        <Button fx:id="confirmButton" layoutX="54.0" layoutY="181.0" mnemonicParsing="false" onAction="#onConfirmButtonClick" text="Confirm" />
        <Button fx:id="cancelButton" layoutX="248.0" layoutY="181.0" mnemonicParsing="false" onAction="#onCancelButtonClick" text="Cancel" />
      <Label layoutX="33.0" layoutY="109.0" text="StartDate" />
      <Label layoutX="191.0" layoutY="109.0" text="DueDate" />
      <HBox layoutX="23.0" layoutY="134.0" prefHeight="26.0" prefWidth="336.0">
         <children>
            <DatePicker fx:id="startDatePicker" />
            <DatePicker fx:id="dueDatePicker" />
         </children>
      </HBox>
    </children>
</AnchorPane>
