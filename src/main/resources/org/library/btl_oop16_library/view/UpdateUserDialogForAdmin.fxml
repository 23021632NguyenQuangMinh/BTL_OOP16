                  <Insets left="30.0" right="30.0" top="15.0" />
               </VBox.margin>
            </ChoiceBox>
         </children>
      </VBox>
      <Button fx:id="cancelButton" layoutX="247.0" layoutY="350.0" mnemonicParsing="false" onAction="#onCancelButtonClick" prefHeight="34.0" prefWidth="110.0" text="Cancel" />
      <Button fx:id="confirmButton" layoutX="100.0" layoutY="350.0" mnemonicParsing="false" onAction="#onConfirmButtonClick" prefHeight="34.0" prefWidth="110.0" text="Confirm " />
