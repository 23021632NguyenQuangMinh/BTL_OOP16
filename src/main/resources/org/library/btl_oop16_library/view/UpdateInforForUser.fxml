<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="253.0" prefWidth="454.0" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.library.btl_oop16_library.Controller.UpdateUserDialogController">
   <opaqueInsets>
      <Insets top="10.0" />
   </opaqueInsets>
   <children>
      <VBox layoutY="-6.0" prefHeight="216.0" prefWidth="454.0">
         <children>
            <TextField fx:id="nameField" layoutX="10.0" layoutY="10.0" maxWidth="385.0" prefHeight="25.0" promptText="Name">
               <VBox.margin>
                  <Insets left="30.0" right="30.0" top="15.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="emailField" layoutX="10.0" layoutY="36.0" maxWidth="385.0" prefHeight="25.0" promptText="Email">
               <VBox.margin>
                  <Insets left="30.0" right="30.0" top="15.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="phoneField" layoutX="40.0" layoutY="145.0" maxWidth="385.0" prefHeight="25.0" promptText="Phone">
               <VBox.margin>
                  <Insets left="30.0" right="30.0" top="15.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="addressField" layoutX="40.0" layoutY="185.0" maxWidth="385.0" prefHeight="25.0" promptText="Address">
               <VBox.margin>
                  <Insets left="30.0" right="30.0" top="15.0" />
               </VBox.margin>
            </TextField>
            <PasswordField fx:id="passwordField" maxWidth="385.0" prefHeight="25.0" prefWidth="376.0" promptText="Enter your pass word to proceed">
               <VBox.margin>
                  <Insets left="30.0" right="30.0" top="15.0" />
               </VBox.margin>
            </PasswordField>
         </children>
      </VBox>
      <Button fx:id="confirmButton" layoutX="165.0" layoutY="205.0" mnemonicParsing="false" onAction="#updateInfor" prefHeight="34.0" prefWidth="110.0" text="Confirm " />
   </children>
</AnchorPane>
