<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="242.0" prefWidth="454.0" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.library.btl_oop16_library.AddUserDialogController">
   <opaqueInsets>
      <Insets top="10.0" />
   </opaqueInsets>
   <children>
      <VBox layoutY="23.0" prefHeight="172.0" prefWidth="454.0">
         <children>
            <TextField fx:id="nameField" layoutX="10.0" layoutY="10.0" maxWidth="385.0" prefHeight="25.0" promptText="Name">
               <VBox.margin>
                  <Insets left="30.0" right="30.0" top="15.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="accountField" maxWidth="385.0" prefHeight="25.0" promptText="Account">
               <VBox.margin>
                  <Insets left="30.0" right="30.0" top="15.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="passwordField" maxWidth="385.0" prefHeight="25.0" promptText="Password">
               <VBox.margin>
                  <Insets left="30.0" right="30.0" top="15.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="emailField" layoutX="10.0" layoutY="36.0" maxWidth="385.0" prefHeight="25.0" promptText="Email">
               <VBox.margin>
                  <Insets left="30.0" right="30.0" top="15.0" />
               </VBox.margin>
            </TextField>
         </children>
      </VBox>
      <Button fx:id="cancelButton" layoutX="249.0" layoutY="195.0" mnemonicParsing="false" onAction="#onCancelButtonClick" prefHeight="34.0" prefWidth="110.0" text="Cancel" />
      <Button fx:id="confirmButton" layoutX="107.0" layoutY="195.0" mnemonicParsing="false" onAction="#onConfirmButtonClick" prefHeight="34.0" prefWidth="110.0" text="Confirm " />
   </children>
</AnchorPane>
